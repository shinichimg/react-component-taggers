{
  "name": "react-component-taggers",
  "version": "0.1.5",
  "description": "A Vite plugin that automatically injects metadata attributes into JSX/TSX elements for debugging and tooling",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "private": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --sourcemap",
    "dev": "tsup src/index.ts --format esm --dts --sourcemap --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vite",
    "plugin",
    "jsx",
    "tsx",
    "react",
    "debugging",
    "metadata",
    "data-attributes"
  ],
  "author": "Phan Hoang Viet",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.23.0",
    "@babel/generator": "^7.23.0",
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "@babel/types": "^7.23.0",
    "estree-walker": "^3.0.3",
    "magic-string": "^0.30.19"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.0",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__traverse": "^7.20.0",
    "@types/node": "^20.0.0",
    "tsup": "^7.0.0",
    "typescript": "^5.0.0",
    "vite": "^4.0.0"
  },
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0 || ^6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shinichimg/react-component-taggers.git"
  },
  "bugs": {
    "url": "https://github.com/shinichimg/react-component-taggers/issues"
  },
  "homepage": "https://github.com/shinichimg/react-component-taggers#readme"
}
